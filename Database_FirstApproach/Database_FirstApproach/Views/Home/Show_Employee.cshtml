
<div class="text-center">

    <table class="table">
        <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">Name</th>
                <th scope="col">Image</th>
              
                <th colspan="2" scope="col">Action</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in ViewBag.employee)
            {
                <tr>
                    <td>@item.EId</td>
                    <td>@item.EName</td>
                    <td><img src="~/Files/@item.EImage"  class="img-thumbnail"/></td>

                    <td> <a asp-action="Emp_del" class="btn btn-outline-danger" asp-route-empId="@item.EId">Delete</a></td>

                    <td> <a asp-action="Emp_edit" class="btn btn-outline-primary" asp-route-empId="@item.EId">Edit</a></td>
                </tr>
            }


        </tbody>
    </table>

</div>
