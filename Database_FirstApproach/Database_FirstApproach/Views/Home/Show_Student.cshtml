@{
    ViewData["Title"] = "Privacy Policy";
}

<div class="text-center">

<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Name</th>
            <th scope="col">Email</th>
            <th scope="col">Phone</th>
            <th scope="col">City</th>
            <th scope="col">Gender</th>
            <th scope="col">Age</th>
            <th colspan="2" scope="col">Action</th>
        </tr>
    </thead>
    <tbody>
            @foreach (var item in ViewData["std_data"] as IEnumerable<Database_FirstApproach.Models.Student>)
            {
                 <tr>
                    <td>@item.SId</td>
                    <td>@item.SName</td>
                    <td>@item.SEmail</td>
                    <td>@item.SPhone</td>
                    <td>@item.SCity</td>
                    <td>@item.SGender</td>
                    <td>@item.SAge</td>
                    <td> <a asp-action="Std_del" class="btn btn-outline-danger" asp-route-stdId="@item.SId">Delete</a></td>

                    <td> <a asp-action="Edit" class="btn btn-outline-primary" asp-route-SId="@item.SId">Edit</a></td>
                </tr> 
            }
       
     
    </tbody>
</table>

</div>
